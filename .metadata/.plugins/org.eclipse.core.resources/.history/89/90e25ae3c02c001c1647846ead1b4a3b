package com.examly.springapp.repository;

import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.examly.springapp.models.ComplaintModel;
import org.springframework.data.repository.CrudRepository;

@Repository
public interface ComplaintRepository extends CrudRepository<ComplaintModel,Integer> {
	ComplaintModel findById(int id);
	//List<ComplaintModel> getissue();
	void deleteComplaintById(Integer id);

	
    Optional<ComplaintModel> findComplaintById(Long id);
	
}
